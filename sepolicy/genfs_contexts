# sysfs
genfscon sysfs /class/android_usb/f_rmnet_smd_sdio/transport                     u:object_r:sysfs_rmnet:s0
genfscon sysfs /devices/virtual/android_usb/android0/f_rmnet_smd_sdio/transport  u:object_r:sysfs_rmnet:s0
genfscon sysfs /module/rmnet_usb/parameters/rmnet_data_init                      u:object_r:sysfs_rmnet:s0

# light
genfscon sysfs /devices/platform/msm_ssbi.0/pm8921-core/pm8xxx-led               u:object_r:sysfs_leds:s0
genfscon sysfs /devices/platform/msm_fb.591617/leds/lcd-backlight                u:object_r:sysfs_leds:s0

# Sysfs files used by mpdecision
genfscon sysfs /module/pm2/modes                                        u:object_r:sysfs_mpdecision:s0
genfscon sysfs /module/pm_8x60/modes                                    u:object_r:sysfs_mpdecision:s0

# Sysfs files used by surfaceflinger
genfscon sysfs /devices/virtual/graphics/fb1/format_3d                  u:object_r:sysfs_surfaceflinger:s0
genfscon sysfs /devices/virtual/graphics/fb1/hpd                        u:object_r:sysfs_surfaceflinger:s0

# Sysfs entry for audio
genfscon sysfs /kernel/boot_adsp/boot                                   u:object_r:sysfs_audio:s0
genfscon sysfs /devices/system/soc/soc0                                 u:object_r:sysfs_soc:s0

# debugfs
genfscon debugfs /kgsl/proc                                             u:object_r:debugfs_kgsl:s0
genfscon debugfs /rpm_stats 						u:object_r:debugfs_rpm:s0
genfscon debugfs /rpm_master_stats 					u:object_r:debugfs_rpm:s0

# Graphics
genfscon sysfs /devices/platform/kgsl-3d0.0/kgsl/kgsl-3d0/gpuclk                 u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/platform/kgsl-3d0.0/kgsl/kgsl-3d0/gpubusy                u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/platform/kgsl-3d0.0/kgsl/kgsl-3d0/max_gpuclk             u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/platform/kgsl-3d0.0/kgsl/kgsl-3d0/gpu_available_frequencies    u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/platform/kgsl-3d0.0/kgsl/kgsl-3d0/reset_count            u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/platform/kgsl-3d0.0/kgsl/kgsl-3d0/pwrscale/trustzone/governor  u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/virtual/graphics/fb0                                     u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/virtual/graphics/fb1                                     u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/virtual/graphics/fb2                                     u:object_r:sysfs_graphics:s0

# thermald
genfscon sysfs /devices/virtual/thermal                                          u:object_r:sysfs_thermal:s0
genfscon sysfs /devices/platform/msm_ssbi.0/pm8921-core/pm8xxx-adc/batt_therm    u:object_r:sysfs_thermal:s0
genfscon sysfs /module/msm_thermal                                               u:object_r:sysfs_thermal:s0




